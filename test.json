          "default": null,
          "title": "Identifier"
        },
        "vendor_metadata": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Vendor Metadata"
        },
        "kind": {
          "const": "binary",
          "default": "binary",
          "title": "Kind",
          "type": "string"
        }
      },
      "required": [
        "data",
        "media_type"
      ],
      "title": "BinaryContent",
      "type": "object"
    },
    "DocumentUrl": {
