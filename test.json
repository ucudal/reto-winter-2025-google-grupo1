    "ModelRequest": {
      "properties": {
        "parts": {
          "items": {
            "discriminator": {
              "mapping": {
                "retry-prompt": "#/$defs/RetryPromptPart",
                "system-prompt": "#/$defs/SystemPromptPart",
                "tool-return": "#/$defs/ToolReturnPart",
                "user-prompt": "#/$defs/UserPromptPart"
              },
              "propertyName": "part_kind"
            },
            "oneOf": [
              {
                "$ref": "#/$defs/SystemPromptPart"
              },
              {
                "$ref": "#/$defs/UserPromptPart"
              },
              {
                "$ref": "#/$defs/ToolReturnPart"
              },
              {
                "$ref": "#/$defs/RetryPromptPart"
              }
            ]
          },
          "title": "Parts",
          "type": "array"
