              },
              {
                "$ref": "#/$defs/ToolCallPart"
              },
              {
                "$ref": "#/$defs/ThinkingPart"
              }
            ]
          },
          "title": "Parts",
          "type": "array"
        },
        "usage": {
          "$ref": "#/$defs/Usage"
        },
        "model_name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Model Name"
